                                </a>
                            </li>
                            <li class="nav-item current-page">
                                <a href="/gestion/Home/Informationstagiaire">
                                    <i class="fa-solid fa-user-graduate"></i>
                                    <span>Pré-stagiaire</span>
                                </a>
                            </li>
                            <li class="nav-item">
                                <a href="/gestion/Home/Evaluation">
                                    <i class="fa-solid fa-file-circle-check"></i>
                                    <span>Evaluation stagiaire</span>
                                </a>
                            </li>
                            <li class="nav-item">
                                <a href="/gestion/Home/Informationstagiaire" class="">
                                    <i class="fa-solid fa-clipboard-list"></i>
                                    <span>Suivi</span>
                                </a>
                                <ul>
                                    <li class="sub-nav-item">
                                        <a href="/gestion/Home/CommandFacture">
                                            <i class="fa-regular fa-circle"></i>
                                            <span>Suivi des commandes </span>
                                        </a>
                                    </li>
                                    <li class="sub-nav-item">
                                        <a href="/gestion/Home/ListeAFacturer">
                                            <i class="fa-regular fa-circle"></i>
                                            <span>Facturation</span>
                                        </a>
                                    </li>
                                    <li class="sub-nav-item">
                                        <a href="">
                                            <i class="fa-regular fa-circle"></i>
                                            <span>Reglements</span>
                                        </a>
                                    </li>
                                </ul>
                            </li>
                            <li class="nav-item">
                                <a href="">
                                    <i class="fa-solid fa-user"></i>
                                    <span>Fin de stage</span>
                                </a>
                            </li>
                            <li class="nav-item">
                                <a href="">
                                    <i class="fa-solid fa-user-group"></i>
                                    <span>Listes prof et individu</span>
                                </a>
                            </li>
                        </ul>

                    </div>

                    <!-- proposition commerciale -->
                    <div class="en-btn">
                        <div class="nav-item">
                            <a href="">
                                <i class="fa-solid fa-bullseye"></i>
                                <span>Proposition commerciale</span>
                            </a>
                        </div>
                    </div>
                </div>
            </aside>

            <div class="main-container">


                @RenderBody()
            </div><!-- /.container-fluid -->
        </div><!-- /.main -->
    </div><!-- /.main-container -->
    </div><!-- /.wrap -->
    <!-- pied de pages -->
    <div class="footer">
        <div class="brand-footer-container">
            <a href=""><div class="brand-footer"></div></a>
        </div>
    </div>


    </div><!-- /.content -->
    <script src="~/Scripts/app.js"></script>

    @Scripts.Render("~/Content/Scripts/Export.js")
    @Scripts.Render("~/Content/Scripts/SendMail.js")
