                    <div class="user-aside-container">
                        <div class="user-aside">
                            <div class="avatar-aside">
                                <img src="~/Content/images/user.png" alt="" class="img-radius">
                                <span class="status online"></span>
                            </div>
                            <span>Admin</span>
                        </div>

                    </div>

                    <!-- menu de navigation -->
                    <div class="nav-container mb-3">

                        <ul class="nav-aside">
                            <li class="nav-item">
