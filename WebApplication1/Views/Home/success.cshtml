@model WebApplication1.Models.StagiaireModel
<!DOCTYPE html>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.css" />
<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.js"></script>
<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title></title>
</head>
<body>
    <form id="form1" runat="server">
        <div>
            <div class="card bg-transparent">
                <div class="container">
                    <div class="login-wrap">
                        <div class="login-content">
                         
                            <div class="login-form">
                                @using (Html.BeginForm())
                                {
                                    @Html.AntiForgeryToken()

                                   
                                        <div class="form-group"hidden>
                                            @Html.EditorFor(model => model.Result, new { htmlAttributes = new { @class = "form-control" } })
                                            @Html.ValidationMessageFor(model => model.Result, "", new { @class = "text-danger" })

                                        </div>
                                        <div class="form-group ">
                                            <p class="label-numero">Nom:</p>
                                            @Html.EditorFor(model => model.nom, new { htmlAttributes = new { @class = "form-control" } })
                                            @Html.ValidationMessageFor(model => model.nom, "", new { @class = "text-danger" })
                                        </div>
                                        <div class="form-group ">
                                            <p class="label-numero">Prenom:</p>
                                            @Html.EditorFor(model => model.prenom, new { htmlAttributes = new { @class = "form-control" } })
                                            @Html.ValidationMessageFor(model => model.prenom, "", new { @class = "text-danger" })
                                        </div>
                                        <div class="form-group ">
                                            <p class="label-numero">Société:</p>
                                            @Html.EditorFor(model => model.societe, new { htmlAttributes = new { @class = "form-control" } })
                                            @Html.ValidationMessageFor(model => model.societe, "", new { @class = "text-danger" })
                                        </div>
                                        <div class="form-group ">
                                            <p class="label-numero">Téléphone:</p>
                                            @Html.EditorFor(model => model.Telephone, new { htmlAttributes = new { @class = "form-control" } })
                                            @Html.ValidationMessageFor(model => model.Telephone, "", new { @class = "text-danger" })
                                        </div>
                                        <div class="form-group ">
                                            <p class="label-numero">Adresse mail:</p>
                                            @Html.EditorFor(model => model.mail, new { htmlAttributes = new { @class = "form-control" } })
                                            @Html.ValidationMessageFor(model => model.mail, "", new { @class = "text-danger" })
                                        </div>

                                        <button class="btn btn-login" type="submit">Sign In</button>
                                    
                                }
                            </div>

                        </div>
                    </div>
                </div>
            </div>
            <footer>
                <div class="navbar-bottom">
                    <div class="row">
                        <div class="col-sm-6 col-xs-6">
                            <a href="./"><i class="material-icons">input</i>Sign in</a>
                        </div>
                        <div class="col-sm-6 col-xs-6">
                            <a href="" id="nav-menu"><i class="material-icons">info</i>Register</a>
                        </div>

                    </div>
                </div>
            </footer>
            <script type="text/javascript">
                setTimeout(function () {
                    swal({
                        title: "Félicitations",
                        text: "verifier votre email nous avons envoyé votre numero de dossier",
                        type: "success"
                    }, function () {
                        window.location = "Index";
                    });
                }, 1000);
            </script>
        </div>
    </form>
</body>
</html>



